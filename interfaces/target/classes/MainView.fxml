<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.MainController" spacing="10">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <Button text="Mostrar Productos" onAction="#mostrarProductos" />
    <Button text="Mostrar Empleados" onAction="#mostrarEmpleados" />
    <Button text="Mostrar Ventas" onAction="#mostrarVentas" />
    <TextArea fx:id="resultadoArea" editable="false" wrapText="true" />
</VBox>